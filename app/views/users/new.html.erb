<h1>New User Form</h1>

<%= form_for @user, url: sessions_create_path(@user), method: :post do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %><br>

  <%= f.label :email %>
  <%= f.text_field :email %><br>
  
  <%= f.label :password %>
  <%= f.password_field :password %><br>

  <%= f.submit 'Login' %>
<% end %>